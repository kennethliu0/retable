{
  "manifest_version": 3,
  "name": "Retable",
  "version": "1.0",
  "description": "Filter HTML tables on any webpage.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Retabler"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}
